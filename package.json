{
	"name": "@zbyteio/zbyte-common",
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/Zbyteio"
	},
	"version": "1.1.0",
	"description": "The zbyte-common package is an essential part of the zbyte SDKs ecosystem, providing a centralized repository of configurations, types/interfaces, utility functions, and more. Its purpose is to ensure consistency and reusability across the various zbyte SDKs, streamlining the development process and enhancing maintainability.",
	"main": "dist/bundle.js",
	"browser": "dist/bundle.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "webpack",
		"test": "npx jest",
		"check-fmt": "npx prettier --check '{src,tests}/**/*.{js,ts}'",
		"format": "prettier --write '{src,tests}/**/*.{js,ts}'",
		"lint": "eslint src",
		"prepare": "husky install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Zbyteio/zbyte-common.git"
	},
	"keywords": [
		"zbyte-common",
		"zbyte-wallet-interface"
	],
	"files": [
		"dist"
	],
	"author": "Kshitij Chaurasiya",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Zbyteio/zbyte-common/issues"
	},
	"homepage": "https://github.com/Zbyteio/zbyte-common#readme",
	"dependencies": {
		"axios": "^1.4.0",
		"lodash": "^4.17.21",
		"prettier": "^2.8.7"
	},
	"devDependencies": {
		"@types/jest": "^29.4.0",
		"@types/lodash": "^4.14.201",
		"@typescript-eslint/eslint-plugin": "^5.53.0",
		"@typescript-eslint/parser": "^5.53.0",
		"eslint": "^8.34.0",
		"husky": "^8.0.3",
		"jest": "^29.4.3",
		"ts-jest": "^29.0.5",
		"ts-loader": "^9.4.2",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5",
		"webpack": "^5.76.0",
		"webpack-cli": "^5.0.1"
	}
}
